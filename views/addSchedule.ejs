<!-- include header -->
<%- include('include/_adminHeader') %>
<!-- /include header -->

<!-- Main Site -->
<!--allows an admin to create a new schedule-->
<div class="adminPage">
  <div class="userHdr">Add Schedule</div>
  <input type="text" id="scheduleName" placeholder="Name" />
  <div id="periods"></div>
  <input class="addPeriod" type="button" value="Add Period" />
  <div>
    <label>Monday</label>
    <input type="checkbox" class="day" />
    <label>Tuesday</label>
    <input type="checkbox" class="day" />
    <label>Wednesday</label>
    <input type="checkbox" class="day" />
    <label>Thursday</label>
    <input type="checkbox" class="day" />
    <label>Friday</label>
    <input type="checkbox" class="day" />
  </div>
  <input class="submit" type="submit" value="Save Schedule" />
</div>

<!-- /Main Site -->

<script src="/js/addSchedule.js"></script>

<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->
