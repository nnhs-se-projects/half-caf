<!-- Main Site -->

<div class="adminPage">
  <div class="userHdr">Delete Topping</div>
  <select id="toDelete">
    <% for (let topping of toppings) { %>
    <option value="<%= topping.id %>"><%= topping.topping %></option>
    <% } %>
  </select>
  <input id="delTopping" type="submit" onclick="deleteTopping() value="Delete
  Topping" />
</div>

<!-- /Main Site -->

<script src="/js/deleteTopping.js"></script>

